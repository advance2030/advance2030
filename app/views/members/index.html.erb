<% content_for :stylesheets do %>
	<%= stylesheet_link_tag 'members2030' %>
<% end %>

<h1>20/30 Members</h1>
<div id="loginSection">
  <% if !@current_user.nil? and @current_user.logged_in? %>
		
		<span>Viewing 1-20 of 300 members</span> 
		<hr />
		
		<%= render :partial => "members_list", 
	  		:collection => @members,
	   		:as => :member %>
	   		
	<% else %>

		<p>You must be logged in to see the members</p>

		

	<% end %>
</div>
